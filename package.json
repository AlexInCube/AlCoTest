{
  "name": "aicbot",
  "version": "4.0.0-dev-5",
  "description": "Discord Bot for playing music",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "development": "tsc&& cross-env NODE_ENV=development node --trace-deprecation build/main.js ",
    "production": "cross-env NODE_ENV=production node build/main.js",
    "test": "tsc&& cross-env NODE_ENV=development node --test",
    "eslint": "eslint \"src/**/*.{ts,js}\"",
    "prettier:format": "prettier --write \"**/*.{ts,js}\"",
    "prettier:check": "prettier --check \"**/*.{ts,js}\""
  },
  "type": "module",
  "keywords": [],
  "author": "AlexInCube",
  "license": "ISC",
  "engines": {
    "node": ">=20"
  },
  "overrides": {
    "uri-js": "npm:uri-js-replace"
  },
  "dependencies": {
    "@discordjs/rest": "2.4.0",
    "@discordjs/voice": "0.17.0",
    "cross-env": "7.0.3",
    "discord.js": "14.16.3",
    "dotenv": "16.4.5",
    "i18next": "23.15.2",
    "i18next-fs-backend": "2.3.2",
    "mongoose": "8.6.3",
    "node-os-utils": "1.3.7",
    "opusscript": "0.1.1",
    "prism-media": "1.3.5",
    "riffy": "https://pkg.pr.new/riffy-team/riffy@bcaaf04",
    "sodium-native": "4.2.1",
    "uuid": "10.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "9.12.0",
    "@types/node": "22.7.5",
    "@types/node-os-utils": "1.3.4",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "@typescript-eslint/parser": "8.8.1",
    "eslint": "9.12.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.11.0",
    "prettier": "3.3.3",
    "typescript": "5.6.3",
    "typescript-eslint": "8.8.1"
  }
}
